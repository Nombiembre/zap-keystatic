---
import Head from '@/components/common/Head.astro';
import Header from '@/components/common/Header.astro';


export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<Head title={title} /> 
	<body class="bg-[#fbfaf5]">
		<Header />
		<slot />
	</body>
</html>